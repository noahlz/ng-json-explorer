.nv-ui-json-explorer
    font-family: monospace;
    font-size: 1.1em;
    white-space: pre-wrap;

    .prop
        font-weight bold

    .null
        color red

    .bool, .num
        color blue

    .string
        color green
        white-space pre-wrap

    .collapser
        position: absolute;
        left: -1em;
        cursor: pointer;
        -moz-user-select: none;
        color: rgb(249, 39, 83);

    .q
        display inline-block
        width 0
        color transparent

    li
        position relative

    ul
        list-style none
        margin 0 0 0 2em
        padding 0

    .error
        -moz-border-radius: 8px;
        border: 1px solid #970000;
        background-color: #F7E8E8;
        margin: .5em;
        padding: .5em;

    .errormessage
        font-family monospace

    h1
        font-size 1.2em

    .callback + #json
        padding-left 1em


    .callback
        font-family monospace
        color #A52A2A

    .hide
        display none


